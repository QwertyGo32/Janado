<!doctype html>
<html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.css">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }} <!-- Header hook for plugins -->
  {{ 'style.css' | asset_url | stylesheet_tag }}
  {{ 'application.scss.css' | asset_url | stylesheet_tag }}
  {{ 'single.css' | asset_url | stylesheet_tag }}
  {{ 'mystyle.css' | asset_url | stylesheet_tag }}
  {{ 'product-cart.css' | asset_url | stylesheet_tag }}
  <script src="//ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script
            src="https://code.jquery.com/jquery-3.4.1.js"
            integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
            crossorigin="anonymous">
    </script>
    {% include 'fa_translate_core' %}
    <style>

    </style>
 </head>
<body>

 {% comment %} {% for link in linklists.main-menu.links %}
    {% assign child_list_handle = link.title | handleize %}
    {% if linklists[child_list_handle].links != blank %}
      <a href="{{ link.url }}">{{ link.title }}</a>
      [{% for childlink in linklists[child_list_handle].links %}
        <a href="{{ childlink.url }}">{{ childlink.title | escape }}</a>
      {% endfor %}]
    {% else %}
      <a href="{{ link.url }}">{{ link.title }}</a>
    {% endif %}
  {% endfor %}

  <a href="/cart">cart</a>

  {% if shop.customer_accounts_enabled %}
    {% if customer %}
      <a href="/account">account</a>
      {{ 'log out'  | customer_logout_link }}
    {% else %}
      {{ 'log in ' | customer_login_link }}
      {{ 'register' | customer_register_link }}
    {% endif %}
  {% endif %}
   {% endcomment %}
{% section 'header' %}
  <main role="main">
    {{ content_for_layout }}
  </main>
 {% section 'footer' %}
 <script src="https://unpkg.com/swiper/swiper-bundle.js"></script>
 {{ 'application.js' | asset_url | script_tag }}
 {{ 'main.js' | asset_url | script_tag }}
 {%- if request.page_type contains 'product' -%}
    {{ 'product.js' | asset_url | script_tag }}
 {% endif %}
 <script src="{{ 'script.js' | asset_url }}" defer></script>


</body>
</html>
