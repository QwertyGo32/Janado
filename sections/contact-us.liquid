<section class="contact">    <video autoplay muted loop class="contact__video" width="100%" height="auto" preload="auto">        <source src="../assets/PexelsVideos2625.mp4" type="video/mp4">    </video>    <div class="contact__wrapper"><h2 class="contact__wrapper_title">{{ section.settings.title }}</h2>        <div class="contact__wrapper_container contact-container">            {% form 'contact', class: 'contact-container__form' %}                {%- if form.errors -%}                    <div class="alert alert--error">                        <span class="alert__title">{{ 'general.forms.errors' | t }}</span>                        {{ form.errors | default_errors }}                    </div>                {%- endif -%}                <div class="contact-container__form_email-name">                    <fieldset class="contact-container__form_name-wrapper">                        <legend class="contact-container__form_name-legend">Your name*</legend>                        <input required type="text" name="contact[first_name]"                               class="contact-container__form_name-input"></fieldset>                    <fieldset class="contact-container__form_email-wrapper">                        <legend class="contact-container__form_email-legend">Your email*</legend>                        <input required type="email" name="contact[email]" class="contact-container__form_email-input">                    </fieldset>                </div>                <fieldset class="contact-container__form_textarea-wrapper">                    <legend class="contact-container__form_textarea-legend">Your message</legend>                    <textarea name="contact[body]" id="" cols="30" rows="10"                              class="contact-container__form_textarea-input"                              form="contact"                              placeholder="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Diam donec imperdiet ut gravida vel nulla quam varius. Egestas ipsum sapien consequat volutpat. Ut egestas ut vitae lorem tempus..."></textarea>                </fieldset>                {%- if form.posted_successfully? -%}                    <p class="form-message form-message--success contact-container__form_text" tabindex="-1"                       data-form-status>                        {{ success_message | default: 'contact.form.post_success' | t }}                    </p>                {%- endif -%}                {% if section.settings.text_btn != blank %}                    <button type="submit" class="contact-container__form_btn">                        <p class="contact-container__form_btn-text">{{ section.settings.text_btn }}</p>                        <svg width="17" height="11" viewBox="0 0 17 11" fill="none" xmlns="http://www.w3.org/2000/svg">                            <path fill-rule="evenodd" clip-rule="evenodd"                                  d="M0.987305 5.26758C0.987305 5.06804 1.0901 4.87667 1.27307 4.73557C1.45604 4.59447 1.70421 4.5152 1.96297 4.5152H13.267L9.07753 1.28601C8.89433 1.14473 8.79141 0.953121 8.79141 0.753326C8.79141 0.553532 8.89433 0.36192 9.07753 0.220644C9.26074 0.0793682 9.50922 0 9.76831 0C10.0274 0 10.2759 0.0793682 10.4591 0.220644L16.3131 4.7349C16.4039 4.80479 16.476 4.88781 16.5252 4.97922C16.5744 5.07062 16.5997 5.16862 16.5997 5.26758C16.5997 5.36654 16.5744 5.46453 16.5252 5.55594C16.476 5.64735 16.4039 5.73037 16.3131 5.80026L10.4591 10.3145C10.2759 10.4558 10.0274 10.5352 9.76831 10.5352C9.50922 10.5352 9.26074 10.4558 9.07753 10.3145C8.89433 10.1732 8.79141 9.98163 8.79141 9.78183C8.79141 9.58204 8.89433 9.39043 9.07753 9.24915L13.267 6.01995H1.96297C1.70421 6.01995 1.45604 5.94069 1.27307 5.79959C1.0901 5.65849 0.987305 5.46712 0.987305 5.26758Z"                                  fill="white"/>                        </svg>                    </button>                {% endif %}            {% endform %}            <div class="contact-container__info">                <div class="contact-container__info_item">                    <p class="contact-container__info_item-title">{{ section.settings.mail }}</p>                    <p class="contact-container__info_item-text">{{ section.settings.adres }}</p>                </div>                <div class="contact-container__info_item">                    <p class="contact-container__info_item-title">{{ section.settings.question }}</p>                    <a href="{{ section.settings.first_link }}"                       class="contact-container__info_item-link">{{ section.settings.first_text }}</a>                    <a href="{{ section.settings.second_link }}"                       class="contact-container__info_item-link">{{ section.settings.second_text }}</a>                    <a href="{{ section.settings.third_link }}"                       class="contact-container__info_item-link">{{ section.settings.third_text }}</a>                </div>            </div>        </div>    </div></section>{% style %}    .contact {    background: rgba(22, 22, 22, 0.3) url({{ section.settings.back_image | img_url: 'master' }}) no-repeat center/cover;    }    .contact-container__form_btn {    background: {{ section.settings.btn_background }}    }    .contact-container__form_btn:hover {    background:{{ section.settings.btn_hover }}    }{% endstyle %}{% schema %}{"name": "Contact Us","settings": [{"type": "text","id": "title","label": "Title"},{"type": "image_picker","id": "back_image","label": "Image"},{"type": "text","id": "text_btn","label": "Button Text"},{"type": "color","id": "btn_background","label": "Button color","default": "#E6D50E"},{"type": "color","id": "btn_hover","label": "Button Hover","default": "#E6D50E"},{"type": "text","id": "mail","label": "Mail Adres"},{"type": "textarea","id": "adres","label": "Adres"},{"type": "text","id": "question","label": "Question Text"},{"type": "text","id": "first_text","label": "First Text "},{"type": "url","id": "first_link","label": "Link to First"},{"type": "text","id": "second_text","label": "Second Text "},{"type": "url","id": "second_link","label": "Link to Second"},{"type": "text","id": "third_text","label": "Third Text "},{"type": "url","id": "third_link","label": "Link to Third"}]}{% endschema %}